{
    "title": "My Notes",
    "author": "xianian",
    "authorHomepage": "",
    "description": "个人笔记",
    "language": "zh-hans",
    "copyright": "All Rights Reserved",
    "root": ".",
    "structure": {
        "summary": "SUMMARY.md",
        "readme": "Introduction.md"
    },
    "variables": {
        "uxDisplayServe": "http://xxx.com",
        "homePage": "/"
    },
    "plugins": [
        "page-toc",
        "theme-default",
        "splitter",
        "prism",
        "folding-chapters",
        "-sharing",
        "url-embed",
        "custom-js-css"
    ],
    "pluginsConfig": {
        "fontsettings": {
            "theme": "sepia",
            "family": "sans",
            "size": 1
        },
        "theme-default": {
            "showLevel": true
        },
        "page-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "before-first",
            "showByDefault": true
        },
        "custom-js-css": {
            "js": [],
            "css": [
                "./css/custom-gitbook.css"
            ]
        }
    }
}