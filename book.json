{
    "title": "Nian",
    "author": "bryson",
    "authorHomepage": "",
    "description": "笔记",
    "language": "zh-hans",
    "copyright": "All Rights Reserved",
    "root": ".",
    "structure": {
        "summary": "SUMMARY.md",
        "readme": "README.md"
    },
    "variables": {
        "homePage": "/"
    },
    "plugins": [
        "page-toc",
        "theme-default",
        "splitter",
        "prism",
        "folding-chapters",
        "-sharing",
        "url-embed"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "sharing": {
            "facebook": false,
            "twitter": false,
            "google": false,
            "weibo": false,
            "instapaper": false,
            "vk": false,
            "all": []
        },
        "page-toc": {
            "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "before-first",
            "showByDefault": true
        }
    }
}